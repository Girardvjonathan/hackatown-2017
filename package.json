{
  "name": "hackatown",
  "version": "1.0.0",
  "description": "HackaTown APP.",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "build:i18n": "rimraf _translations && BABEL_ENV=i18n babel --quiet lib > /dev/null",
    "build:i18n:langs": "npm run build:i18n && babel-node scripts/i18nToXliff.js",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development npm start",
    "start": "npm run clean && node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Girardvjonathan/hackatown-2017.git"
  },
  "license": "MIT",
